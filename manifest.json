{
    "manifest_version": 3,
    "name": "Tab Title Extension",
    "version": "1.0",
    "description": "An extension to change the title of the current tab.",
    "permissions":  ["tabs", "activeTab", "scripting"],
     "action": {
    "default_popup": "popup.html"
  },
   "host_permissions": ["<all_urls>"],
   "icons": {
   "16": "icon.png",
   "32": "icon.png",
   "48": "icon.png",
   "128": "icon.png"
}
}